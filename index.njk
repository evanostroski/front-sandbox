---
title: Sandbox for sand fox
layout: default
---
<div class="container-fluid">
    <div class="row justify-content-start">
        <div class="col-md-7 py-3">
            <img class="img-fluid rounded mr-2 float-left" src="{{site.img | url}}fennec.png">
        </div>
        <div class="col-md-5 p-2">
            <h1 class="display-1">
                {{site.title}}
            </h1>
            <div class="text-right mt-n3 mb-3">
                <small class="text-muted pr-4">{{title}}</small>
            </div>
            <p class="lead mt-5 pl-4">
                {{site.description}}
            </p>
                      
        </div>
    </div>
</div>
<hr>
<div class="container-fluid">
    <div class="row justify-content-center">
        <div class="col-md-5 m-3 p-3 border border-dark rounded-lg">
            {% set formlist = collections.forms %}
            {% include "formlist.njk" %}
        </div>
        <div class="col-md-5 m-3 p-3 border border-dark rounded-lg">
            {% set chatlist = collections.chats %}
            {% include "chatlist.njk" %}
        </div>
    </div>
</div>